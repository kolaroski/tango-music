{
  "private": true,
  "name": "tango-music",
  "description": "Web framework for tango music",
  "scripts": {
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build": "webpack --progress --mode=production",
    "start": "webpack s --config ./webpack.config.js --mode=development",
    "start:prod": "npm run build && http-server dist -p 9999 -o"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kolaroski/tango-music.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kolaroski/tango-music/issues"
  },
  "homepage": "https://github.com/kolaroski/tango-music#readme",
  "devDependencies": {
    "@babel/core": "7.17.5",
    "@babel/plugin-proposal-class-properties": "7.16.7",
    "@babel/plugin-proposal-object-rest-spread": "7.17.3",
    "@babel/preset-env": "7.16.11",
    "@babel/preset-react": "7.16.7",
    "@babel/preset-typescript": "7.16.7",
    "babel-loader": "8.2.3",
    "css-loader": "^6.7.1",
    "fork-ts-checker-webpack-plugin": "7.2.1",
    "html-webpack-plugin": "5.5.0",
    "http-server": "14.1.0",
    "mini-css-extract-plugin": "^2.6.0",
    "typescript": "4.6.2",
    "webpack": "5.70.0",
    "webpack-cli": "4.9.2",
    "webpack-dev-server": "4.7.4"
  },
  "dependencies": {
    "@types/react": "17.0.39",
    "@types/react-dom": "17.0.13",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  }
}
